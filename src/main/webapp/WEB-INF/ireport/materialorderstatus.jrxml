	<parameter name="SYS_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT   A.SYS_ID           AS "sysId",
         A.SHET_DATE		AS "shetDate",
		 A.SHET_NO 			AS "shetNo",
		 A.CUST_NAME 		AS "custName",
		 B.ITEM_NAME 		AS "itemName",
		 B.ITEM_SPEC		AS "itemSpec",
		 B.ITEM_UNIT 		AS "itemUnit",
		 B.ITEM_QTY 		AS "itemQty",
		 B.ITEM_PRCE		AS "itemPrce",
		 B.TOT_PRCE 		AS "itemCost"
from SHET_MAST A, SHET_DETL B
WHERE A.SYS_ID    = $P{SYS_ID}
	AND A.SYS_ID = B.SYS_ID
	AND A.SHET_TYPE = B.SHET_TYPE
	AND A.SHET_NO = B.SHET_NO
	AND A.SHET_TYPE='R'
	AND A.USE_IDX='Y'
	AND B.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
	AND A.CUST_CODE   LIKE CONCAT('%',$P{searchcustCode},'%')
	AND A.CUST_NAME   LIKE CONCAT('%',$P{searchcustName},'%')
	AND B.ITEM_NAME   LIKE CONCAT('%',$P{searchitemName},'%')
	AND B.ITEM_SPEC   LIKE CONCAT('%',$P{searchitemSpec},'%')
GROUP
	BY B.SHET_NO, B.SHET_SEQ, A.CUST_CODE
ORDER
	BY B.SHET_DATE, B.SHET_SEQ ASC]]>
	</queryString>
