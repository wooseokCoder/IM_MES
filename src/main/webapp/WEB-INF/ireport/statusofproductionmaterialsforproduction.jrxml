	<queryString>
		<![CDATA[SELECT
A.SYS_ID    AS "sysId"
	,A.SHET_DATE AS "shetDate"
	,A.ITEM_CODE AS "itemCode"
	,B.STAF_DEPT AS "stafDept"
	,C.ITEM_TYPE1 AS "itemType1"
	,C.ITEM_TYPE2 AS "itemType2"
	,A.ITEM_NAME AS "itemName"
	,A.ITEM_SPEC AS "itemSpec"
	,A.ITEM_UNIT AS "itemUnit"
	,A.ITEM_QTY AS "itemQty"
	,A.ITEM_PRCE AS "itemPrce"
	,A.TOT_AMT AS "totAmt"
	,(SELECT C.CODE_NAME
		  FROM SYS_CODE C
		 WHERE SYS_ID = 'WSC'
		   AND C.CODE_GRUP = 'DEPT_CODE'
		   AND C.CODE_CD = B.STAF_DEPT) AS "stafDeptName"
FROM SHET_DETL A, SHET_MAST B, ITEM_MAST C
	WHERE
		A.SYS_ID    = 'WSC'
    		AND B.SHET_TYPE='D'
    		AND A.SHET_NO=B.SHET_NO
    		AND A.ITEM_CODE=C.ITEM_CODE
GROUP BY C.ITEM_TYPE1]]>
	</queryString>
