	<queryString>
		<![CDATA[SELECT
         B.SHET_DATE	AS "shetDate"
        ,B.SHET_NO	AS "shetNo"
        ,A.CUST_NAME    AS "custName"
        ,B.ITEM_NAME	AS "itemName"
        ,B.ITEM_SPEC	AS "itemSpec"
        ,B.ITEM_UNIT	AS "itemUnit"
        ,B.ITEM_PRCE    AS "itemPrce"
        ,B.ITEM_QTY	AS "itemQty"
        ,B.ITEM_COST    AS "itemCost"
        ,B.TOT_TAX		AS "totTax"
        ,B.TOT_AMT		AS "totAmt"
    FROM SHET_MAST A	  ,SHET_DETL B
	WHERE A.SYS_ID=$P{SYS_ID}
		AND B.SHET_DATE  = $P{shetDate}
    	AND B.SHET_TYPE='P'
		AND A.SHET_TYPE='P'
		AND A.SHET_TYPE=B.SHET_TYPE
    	AND A.SHET_NO=B.SHET_NO
        AND A.PROD_YN	= 'Y'
ORDER BY B.SHET_DATE DESC, B.SHET_NO DESC]]>
	</queryString>
