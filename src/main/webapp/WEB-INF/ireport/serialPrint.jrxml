	<queryString>
		<![CDATA[SELECT
       SYS_ID      		 AS "sysId"
     , SHET_TYPE   		 AS "shetType"
     , SHET_NO     		 AS "shetNo"
     , SHET_SEQ	 	AS "shetSeq"
     , SER_NO		AS "serNo"
     , SER_NO		AS "rserNo"
     , SUBSTRING(REGI_DATE,1,8)	AS "regiDate"
FROM SHET_ITEM_SER A

WHERE A.SYS_ID = $P{SYS_ID}
AND A.SHET_TYPE=$P{shetType}
AND A.SHET_NO=$P{shetNo}
AND A.SHET_SEQ=$P{shetSeq}
AND FIND_IN_SET(A.SER_NO,$P{serNo})]]>
