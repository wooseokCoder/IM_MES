	<queryString>
		<![CDATA[SELECT
	  CONCAT(A.SHET_NO,'::',A.SHET_TYPE) AS "shetNoType"
     , A.SHET_DATE    AS "shetDate"
     , CONCAT(A.SHET_TYPE,A.SHET_NO) AS "estimateNo"
     , (SELECT CD1.CODE_NAME FROM SYS_CODE CD1 WHERE CD1.SYS_ID = A.SYS_ID AND CD1.CODE_CD = A.STAF_DEPT AND CD1.CODE_GRUP = 'DEPT_CODE') AS "stafDeptName"
     , A.STAF_NAME	AS "stafName"
     , (SELECT CD2.CODE_NAME FROM SYS_CODE CD2 WHERE CD2.SYS_ID = A.SYS_ID AND CD2.CODE_CD = A.PAY_TYPE AND CD2.CODE_GRUP = 'PAY_TYPE') AS "payTypeName"
     , A.CUST_NAME    AS "custName"
     , A.SHOP_NAME    AS "shopName"
     , A.SHOP_STAF_NAME AS "shopStafName"
     , A.SHET_AMT     AS "shetAmt"
     , A.SHET_TAX     AS "shetTax"
     , A.SHET_TOT     AS "shetTot"
     , A.REMK1        AS "remk1"
     , A.REMK3        AS "remk3"
     , A.COMP_TEL     AS "compTel"
     , A.COMP_FAX     AS "compFax"
     , A.COMP_MAIL    AS "compMail"
     ,(SELECT CD2.CODE_NAME FROM SYS_CODE CD2 WHERE CD2.SYS_ID = A.SYS_ID AND CD2.CODE_CD = A.UNSTOR_DEPOT AND CD2.CODE_GRUP = 'STRG_TYPE')  AS "unstorDepot"
     , (SELECT COUNT(*) FROM (

		SELECT
		 CONCAT(SHET_NO,'::',SHET_SEQ) AS "shetNoSeq"
		 FROM shet_detl A
		WHERE  SYS_ID    = $P{SYS_ID}
			AND SHET_TYPE='R'
			AND SHET_NO = $P{estimateNo}
		         ORDER BY SHET_NO DESC, SHET_DATE DESC

			) X) AS "itemCount"

   ,sum(b.ITEM_QTY) as "itemQty"
   ,sum(b.TOT_PRCE) as "totPrce"
   ,sum(b.TOT_TAX)  as "totTax"
   ,sum(b.TOT_AMT)  as "totAmt"
   FROM SHET_MAST A, shet_detl B
	WHERE
	a.sys_id = b.sys_id
	and a.SHET_TYPE = b.SHET_TYPE
	and a.SHET_NO = b.SHET_NO
	and A.SYS_ID    = $P{SYS_ID}
    	AND A.SHET_TYPE  = 'R'
    	AND  A.SHET_NO = $P{estimateNo}]]>
	</queryString>
