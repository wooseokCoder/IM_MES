        ,A.CUST_STAF_TEL	AS "custStafTel"
        ,A.CUST_STAF_FAX	AS "custStafFax"
        ,A.CUST_STAF		AS "custStaf"
        ,A.CUST_STAF_HP		AS "custStafHp"
        ,A.DIST_DATE   		AS "distDate"
