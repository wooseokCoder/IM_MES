	<parameter name="t_custCode" class="java.lang.String"/>
	<parameter name="t_custName" class="java.lang.String"/>
	<parameter name="deptCode" class="java.lang.String"/>
	<parameter name="t_state" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
A.INCOME_DATE		AS "incomeDate"
        ,A.SEQ          	AS "incomeSeq"
        ,A.CUST_NAME		AS "custName"
        ,A.CUST_CD			AS "custCd"
        ,A.ITEM_CD			AS "itemCode"
        ,A.ITEM_NAME		AS "itemName"
        ,A.ITEM_SPEC		AS "itemSpec"
        ,A.PROG_STATE		AS "progState"
        ,A.CMPLT_YN			AS "cmpltYn"
        ,A.REQUEST_REASON	AS "requestReason"
        ,A.REMARK			AS "remark"
        ,A.INCOME_PLACE		AS "incomePlace"
        ,A.INCOME_TYPE		AS "incomeType"
        ,A.INCOME_DEPT		AS "incomeDept"
        ,A.BIZ_GP			AS "bizGp"
        ,A.TAX_YN			AS "taxYn"
        ,A.SERIAL			AS "serial"
        ,A.SEV_CHARGE		AS "sevCharge"
        ,A.CUST_STAF_TEL	AS "custStafTel"
        ,A.CUST_STAF_FAX	AS "custStafFax"
        ,A.CUST_STAF		AS "custStaf"
        ,A.CUST_STAF_HP		AS "custStafHp"
        ,A.DIST_DATE   		AS "distDate"
        ,A.DIST_PLACE  		AS "distPlace"
        ,A.AS_COST			AS "asCost"
        ,A.TOT_TAX			AS "totTax"
        ,A.TOT_AMT			AS "totAmt"
        ,A.ISSUER			AS "issuer"
        ,(SELECT SB2.USER_NAME FROM SYS_USER SB2 WHERE SB2.USER_ID = A.REGI_ID  AND SB2.SYS_ID = A.SYS_ID) AS "userName"
FROM AS_MAST A
WHERE A.SYS_ID=$P{SYS_ID}
	AND A.INCOME_DATE BETWEEN  $P{date1} AND $P{date2}
	AND A.CUST_CD
LIKE CONCAT ('%',IF($P{t_custCode} IS NULL,'',$P{t_custCode}),'%')
	AND CASE WHEN $P{deptCode} = "00" THEN 1=1		 		ELSE A.INCOME_DEPT LIKE CONCAT('%',$P{deptCode},'%')
 	END
	AND CASE WHEN $P{t_state} = "A" THEN 1=1		 		WHEN $P{t_state} = "Y" THEN A.CMPLT_YN = 'Y'		 		ELSE IFNULL(A.CMPLT_YN,'N') != 'Y'
 	END

GROUP BY A.INCOME_DATE
  ORDER BY A.INCOME_DATE, A.SEQ ASC]]>
	</queryString>
