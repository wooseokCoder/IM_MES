	<queryString>
		<![CDATA[SELECT
   	 A.SYS_ID    AS "sysId"
	,A.ITEM_QTY AS "itemQty"
	,SUM(A.ITEM_QTY) AS "SumItemQty"
	,SUM(A.TOT_AMT) AS "SumtotAmt"
    FROM SHET_DETL A, SHET_MAST B
	WHERE A.SYS_ID=$P{SYS_ID}
		AND A.SHET_TYPE='P'
		AND B.SHET_TYPE='P'
		AND A.SHET_TYPE=B.SHET_TYPE
    	AND A.SHET_NO=B.SHET_NO
		AND B.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
        AND B.PROD_YN	= 'Y'
ORDER BY B.SHET_DATE DESC, B.SHET_NO DESC]]>
	</queryString>
	<field name="sysId" class="java.lang.String"/>
	<field name="itemQty" class="java.math.BigDecimal"/>
	<field name="SumItemQty" class="java.math.BigDecimal"/>
	<field name="SumtotAmt" class="java.math.BigDecimal"/>
