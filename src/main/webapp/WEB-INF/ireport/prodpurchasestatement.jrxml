	<queryString>
		<![CDATA[SELECT
     A.SYS_ID    AS "sysId"
	,B.SHET_DATE AS "shetDate"
	,A.ITEM_CODE AS "itemCode"
	,B.STAF_DEPT AS "stafDept"
	,A.ITEM_NAME AS "itemName"
	,A.ITEM_SPEC AS "itemSpec"
	,A.ITEM_UNIT AS "itemUnit"
	,A.ITEM_QTY AS "itemQty"
	,A.ITEM_PRCE AS "itemPrce"
	,A.TOT_AMT AS "totAmt"
    FROM SHET_DETL A, SHET_MAST B
	WHERE A.SYS_ID=$P{SYS_ID}
        AND B.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
		AND A.SHET_TYPE='P'
    	AND B.SHET_TYPE='P'
    	AND A.SHET_NO=B.SHET_NO
        AND B.PROD_YN	= 'Y'
GROUP BY B.SHET_DATE
ORDER BY B.SHET_DATE DESC, B.SHET_NO DESC]]>
	</queryString>
