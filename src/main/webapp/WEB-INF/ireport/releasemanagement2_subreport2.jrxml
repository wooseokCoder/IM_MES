	<queryString>
		<![CDATA[SELECT
   	A.SYS_ID       AS "sysId"
     , A.SHET_TYPE    AS "shetType"
     , A.SHET_NO      AS "shetNo"
     , A.SHET_SEQ     AS "shetSeq"
     , A.SHET_DATE    AS "shetDate"
     , A.ITEM_CODE    AS "itemCode"
     , A.ITEM_NAME    AS "itemName"
     , A.ITEM_SPEC    AS "itemSpec"
     , A.ITEM_UNIT    AS "itemUnit"
     , A.ITEM_QTY     AS "itemQty"
     , A.ITEM_PRCE    AS "itemPrce"
     , A.ITEM_PRCE * ITEM_QTY	AS "totPrce"
     , A.ITEM_COST    AS "itemCost"
     , A.TOT_TAX      AS "totTax"
     , A.TOT_AMT      AS "totAmt"
     , A.TOT_PRCE	AS "totPrce"
     , A. base_prce    AS "basePrce"
     , A.DISC_RATE    AS "discRate"
     , A.PURC_PRCE    AS "purcPrce"
     , A.SALE_PRCE    AS "salePrce"
     , A.ON_HAND_QTY  AS "onHandQty"
     , A.LAST_TRAD    AS "lastTrad"
     , A.LAST_PRCE    AS "lastPrce"
     , A.MARGIN_RATE  AS "marginRate"
     , A.CUST_CODE    AS "custCode"
     , A.SHOP_NAME    AS "shopName"
     , A.REMK         AS "remk"
     , A.USE_IDX      AS "useIdx"
     , A.REGI_DATE	AS "regiDate"
     , A.REGI_ID	AS "regiUser"
     , A.CHNG_DATE	AS "updtDate"
     , A.CHNG_ID	AS "updtUser"
     , 'R'          AS "rowStat"
     , CONCAT(A.SHET_NO,'::',A.SHET_SEQ) AS "shetNoSeq"
			 FROM shet_detl A
         WHERE A.SYS_ID    = $P{SYS_ID}
    	AND A.SHET_TYPE  = 'I'
	AND A.SHET_NO   = $P{hShetNo}]]>
	</queryString>
