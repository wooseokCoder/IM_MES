	<parameter name="date1" class="java.lang.String"/>
	<parameter name="date2" class="java.lang.String"/>
	<parameter name="t_custName" class="java.lang.String"/>
	<parameter name="t_custCode" class="java.lang.String"/>
	<parameter name="SYS_ID" class="java.lang.String"/>
	<parameter name="t_sendType" class="java.lang.String"/>
	<parameter name="t_remk4" class="java.lang.String"/>
	<parameter name="dShetType" class="java.lang.String"/>
	<parameter name="dShetNo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
      A.SYS_ID AS "sysId",
      A.SHET_TYPE AS "shetType",
      A.SHET_NO AS "shetNo",
      (SELECT SB.CODE_NAME FROM SYS_CODE SB WHERE SB.CODE_GRUP = 'COLL_PAY_YN' AND SB.CODE_CD = $P{t_remk4}) AS "t_remk4Name",
      (CASE WHEN $P{t_sendType}='0' THEN '현금전표'
            WHEN $P{t_sendType}='1' THEN '대체전표'
            ELSE '전체' END)  AS "t_sendType"
FROM  SHET_MAST A, SHET_DETL B
WHERE
      A.SYS_ID = $P{SYS_ID}
  AND FIND_IN_SET(A.SHET_TYPE,$P{dShetType})
  AND FIND_IN_SET(A.SHET_NO, $P{dShetNo})
  AND A.SYS_ID = B.SYS_ID
  AND A.SHET_TYPE = B.SHET_TYPE
  AND A.SHET_NO = B.SHET_NO
  AND A.SHET_TYPE  = 'C'
  AND A.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
  AND B.CUST_CODE   LIKE CONCAT('%',$P{t_custCode},'%')
  AND B.CUST_NAME   LIKE CONCAT('%',$P{t_custName},'%')
  AND CASE WHEN $P{t_sendType} = '' THEN 1=1  ELSE A.SEND_TYPE LIKE CONCAT('%',$P{t_sendType},'%') END
  AND CASE WHEN $P{t_remk4} = '' THEN 1=1  ELSE A.REMK4 LIKE CONCAT('%',$P{t_remk4},'%') END
GROUP BY A.SYS_ID, A.SHET_TYPE, A.SHET_NO, A.SHET_DATE, A.CUST_CODE, A.CUST_NAME, A.SHET_AMT, A.SHET_TAX, A.SHET_TOT, A.SEND_TYPE, A.PAY_TYPE, A.PAY_DATE, A.REMK4, A.REMK5, A.REMK3, A.REMK2, A.REMK1, A.BIZ_GP, A.STAF_DEPT, A.STAF_NAME, A.DB_AMT, A.REFERENCE_NO, A.DEPT_NAME
ORDER
  BY   A.SHET_NO DESC, A.SHET_DATE DESC]]>
	</queryString>
