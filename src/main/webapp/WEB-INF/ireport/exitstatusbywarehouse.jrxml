	<parameter name="SYS_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    A.SYS_ID           AS "sysId"
   ,B.STOC_LOC	 AS "stocLoc"
	,A.ITEM_CODE AS "itemCode"
	,A.ITEM_NAME AS "itemName"
	,A.ITEM_SPEC AS "itemSpec"
	,A.ITEM_UNIT AS "itemUnit"
	,B.ITEM_TYPE1 AS "itemType1"
	,B.ITEM_TYPE2 AS "itemType2"
	,B.SALE_PRCE AS "salePrce"
	,A.CARRY_QTY AS "carryQty"
	,0		AS "IN"
	,0		AS "OUT"
	,A.CURRENT_QTY AS "currentQty"
	,(SELECT B.CODE_NAME
		  FROM SYS_CODE B
		 WHERE SYS_ID = 'WSC'
		   AND B.CODE_GRUP = 'STRG_TYPE'
		   AND B.USE_FLAG='Y'
		   AND B.CODE_CD = A.STOC_LOC) AS "stocLocName"
	FROM INVT_MAST A ,   	 ITEM_MAST B
 WHERE A.SYS_ID    = $P{SYS_ID}
 AND A.ITEM_CODE = B.ITEM_CODE
 AND A.STOCK_DATE   BETWEEN $P{date1} AND $P{date2}]]>
	</queryString>
