	<parameter name="SYS_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
 		B.SHET_DATE AS "shetDate"
		   ,B.ITEM_CODE  AS "itemCode"
			,B.SHET_NO 	 AS "shetNo"
			,A.STOC_LOC  AS "stocLoc"
			,B.ITEM_NAME AS "itemName"
			,B.ITEM_SPEC AS "itemSpec"
			,B.ITEM_UNIT AS "itemUnit"
			,B.ITEM_QTY  AS "itemQty"
			,B.ITEM_PRCE AS "itemPrce"
			,B.TOT_TAX   AS "totTax"
			,B.TOT_AMT   AS "totAmt"
		   ,B.DISPOSAL_TYPE AS "disposalType"
		   ,B.REMK AS "Remk"
		   ,(SELECT B.CODE_NAME
		  FROM SYS_CODE B
		 WHERE SYS_ID = 'WSC'
		   AND B.CODE_GRUP = 'STRG_TYPE'
		   AND B.USE_FLAG='Y'
		   AND B.CODE_CD = A.STOC_LOC) AS "stocLocName"
		   ,(SELECT B.CODE_NAME
		  FROM SYS_CODE B
		 WHERE SYS_ID = 'WSC'
		   AND B.CODE_GRUP = 'DFTV_DSCD_TYPE'
		   AND B.USE_FLAG='Y'
		   AND B.CODE_CD = B.DISPOSAL_TYPE) AS "disposalTypeName"

		   FROM ITEM_MAST A, SHET_DETL B
		   WHERE A.SYS_ID    = $P{SYS_ID}
    		AND A.ITEM_CODE = B.ITEM_CODE
    		AND DISPOSAL_STATE='Y'
	AND A.STOC_LOC  LIKE CONCAT ('%',IF($P{searchdisposalType} IS NULL,'',$P{searchdisposalType}),'%')
	AND B.DISPOSAL_TYPE LIKE CONCAT ('%',IF($P{searchstocLoc} IS NULL,'',$P{searchstocLoc}),'%')]]>
	</queryString>
