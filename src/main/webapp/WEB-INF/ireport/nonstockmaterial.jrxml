	<parameter name="date1" class="java.lang.String"/>
	<parameter name="date2" class="java.lang.String"/>
	<parameter name="searchcustCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="searchcustName" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="searchitemName" class="java.lang.String"/>
	<parameter name="searchitemSpec" class="java.lang.String"/>
	<parameter name="searchEitem" class="java.lang.String"/>
	<parameter name="SYS_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="searchDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT A.CUST_NAME    AS "custName"
        ,A.SHET_NO    	AS "shetNo"
         , CONCAT(A.SHET_TYPE,A.SHET_NO) AS "estimateNo"
		,A.SHET_DATE	AS "shetDate"
        ,A.PAY_DATE		AS "payDate"
        ,B.ITEM_NAME	AS "itemName"
        ,B.ITEM_SPEC	AS "itemSpec"
        ,B.ITEM_QTY		AS "itemQty"
        ,B.ITEM_QTY     AS "eitemQty"
        ,''				AS "subQty"
        ,''				AS "obtOrdr"
        ,CASE  WHEN B.INPUT_CHECK= 'Y' THEN '완결' ELSE '미결' END	AS "divTrue"
        ,IFNULL(B.INPUT_CHECK,'N') AS "inputCheck"
FROM SHET_MAST A
	  ,SHET_DETL B
WHERE  A.SYS_ID = B.SYS_ID
		AND A.SHET_TYPE = B.SHET_TYPE
		AND A.SHET_NO   = B.SHET_NO
		AND A.SYS_ID = $P{SYS_ID}
    	AND A.USE_IDX	  = 'Y'
    	AND A.SHET_TYPE = 'B'
		AND CASE WHEN $P{searchDate} = "1" THEN A.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
	 			 ELSE A.PAY_DATE   BETWEEN $P{date1} AND $P{date2}
	 		 END
    	AND A.CUST_CODE    like CONCAT('%',$P{searchcustCode},'%')
    	AND A.CUST_NAME   like CONCAT('%',$P{searchcustName},'%')
    	AND B.ITEM_NAME  like CONCAT('%',$P{searchitemName},'%')
    	AND B.ITEM_SPEC   like CONCAT('%',$P{searchitemSpec},'%')
    	AND CASE WHEN $P{searchEitem} = "1" THEN 1=1
				 WHEN $P{searchEitem} = "3" THEN B.INPUT_CHECK ='Y'
	 			 ELSE B.INPUT_CHECK !='Y'
	 		END
ORDER
BY A.CUST_NAME, A.SHET_DATE, B.ITEM_NAME, B.ITEM_SPEC ASC]]>
	</queryString>
