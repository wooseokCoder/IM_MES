AND    SHET_TYPE = 'S'
AND    SHET_DATE BETWEEN DATE_FORMAT( ADDDATE(NOW(), -WEEKDAY(NOW()) -0 ), '%Y-%m-%d')
                     AND DATE_FORMAT( ADDDATE(NOW(), -WEEKDAY(NOW()) +6 ), '%Y-%m-%d')
;]]>
