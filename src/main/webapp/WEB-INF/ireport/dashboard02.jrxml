	<queryString>
		<![CDATA[SELECT '주간' AS S_GROUP, '전주' AS S_TYPE, COUNT(*) AS CNT
FROM   SHET_MAST
WHERE  SYS_ID    = 'WSC'
AND    SHET_TYPE = 'S'
AND    SHET_DATE BETWEEN DATE_FORMAT( ADDDATE(NOW(), -WEEKDAY(NOW()) -7 ), '%Y-%m-%d')
                     AND DATE_FORMAT( ADDDATE(NOW(), -WEEKDAY(NOW()) -1 ), '%Y-%m-%d')
UNION
SELECT '주간' AS S_GROUP, '금주' AS S_TYPE, COUNT(*) AS CNT
FROM   SHET_MAST
