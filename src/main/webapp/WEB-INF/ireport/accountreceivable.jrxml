	<queryString>
		<![CDATA[SELECT
         a.sysID
        ,a.custCode
        ,a.custName AS "custName"
        ,SUM(a.shetAmt) AS "shetAmt"
        ,SUM(a.shetTax) AS "shetTax"
        ,SUM(a.shetTot) AS "shetTot"
        ,SUM(a.TaxshetAmt) as "c_shetTot"
        ,B.COMP_TEL1 AS "compTel"
        ,B.COMP_STAF_HP AS "compHp"
        ,B.STAF_NAME AS "stafName"
        ,B.COMP_FAX  AS "compFax"
        ,0 AS "null2"
        ,SUM(ifnull(a.shetTot,0))-SUM(ifnull(a.TaxshetAmt,0)) AS "null3"
        ,(SELECT SB.CODE_NAME
          FROM SYS_CODE SB
          WHERE     SB.SYS_ID = a.sysID
                AND SB.CODE_CD =  b.CUST_TYPE
                AND SB.CODE_GRUP = 'CUST_TYPE') AS "custType"
        ,CASE WHEN $P{GsearchVat} = '1' THEN ifnull(fn_get_base_misu($P{SYS_ID},a.custCode,DATE_FORMAT($P{date1},'%Y-%m-%d')),0)*1
              WHEN $P{GsearchVat} = '2' THEN ifnull(fn_get_base_miji($P{SYS_ID},a.custCode,DATE_FORMAT($P{date1},'%Y-%m-%d')),0)*1
              ELSE (ifnull(fn_get_base_misu($P{SYS_ID},a.custCode,DATE_FORMAT($P{date1},'%Y-%m-%d')),0)
                   -ifnull(fn_get_base_miji($P{SYS_ID},a.custCode,DATE_FORMAT($P{date1},'%Y-%m-%d')),0))*1
               END AS "blacCari"
        ,CASE WHEN $P{GsearchVat} = '1' THEN ifnull(fn_get_base_misu($P{SYS_ID},a.custCode,DATE_FORMAT($P{date2},'%Y-%m-%d')),0)*1
              WHEN $P{GsearchVat} = '2' THEN ifnull(fn_get_base_miji($P{SYS_ID},a.custCode,DATE_FORMAT($P{date2},'%Y-%m-%d')),0)*1
              ELSE (ifnull(fn_get_base_misu($P{SYS_ID},a.custCode,DATE_FORMAT($P{date2},'%Y-%m-%d')),0)
                    -ifnull(fn_get_base_miji($P{SYS_ID},a.custCode,DATE_FORMAT($P{date2},'%Y-%m-%d')),0))*1
               END AS "unBlac"
        ,CASE WHEN $P{GsearchVat} = '1' THEN '매출액'
              WHEN $P{GsearchVat} = '2' THEN '매입액'
              ELSE '매출액/매입액'
              END AS "TEXT"
  FROM (
        SELECT A.SYS_ID AS sysId,
               A.CUST_CODE  AS custCode ,
               A.CUST_NAME as custName,
               SUM(IFNULL(B.TOT_PRCE,0)) AS "shetAmt",
               SUM(IFNULL(B.TOT_TAX,0)) AS "shetTax",
               SUM(IFNULL(B.TOT_AMT,0)) AS "shetTot",
               0 as "TaxshetAmt"
         FROM SHET_MAST A, SHET_DETL B
        WHERE  A.SYS_ID = $P{SYS_ID}
          AND


              (CASE WHEN $P{GsearchVat} = "1" THEN   A.SHET_TYPE ='S'
                      WHEN $P{GsearchVat}  = "2" THEN   A.SHET_TYPE ='P'
                      WHEN $P{GsearchVat}  = "3" THEN   A.SHET_TYPE IN ('S','P')
              END)
          AND A.SYS_ID = B.SYS_ID
          AND A.SHET_TYPE = B.SHET_TYPE
          AND A.SHET_NO = B.SHET_NO
          AND A.SHET_DATE BETWEEN $P{date1} AND $P{date2}

          AND A.CUST_CODE LIKE CONCAT('%',$P{t_custCode},'%')
          AND A.CUST_NAME LIKE CONCAT('%',$P{t_custName},'%')



    GROUP BY  sysID,custCode
             UNION ALL
              SELECT b.SYS_ID AS sysId,
               b.CUST_CODE AS custCode,
               b.CUST_NAME as custName,
               0 AS "shetAmt",
               0 AS "shetTax",
               0 AS "shetTot",
               CASE WHEN $P{GsearchVat}='1' THEN SUM(ifnull(b.REFER_AMT, 0))
                    WHEN $P{GsearchVat}='2' THEN SUM(ifnull(b.TOT_AMT, 0))
                    WHEN $P{GsearchVat}='3' THEN SUM(ifnull(b.REFER_AMT, 0))+SUM(ifnull(b.TOT_AMT, 0)) END AS "TaxshetAmt"
         FROM shet_detl b, shet_mast a
        WHERE     B.SYS_ID = A.SYS_ID
              AND B.SHET_TYPE =A.SHET_TYPE
              AND B.SHET_NO = A.SHET_NO
              AND b.SYS_ID = $P{SYS_ID}
              AND b.SHET_TYPE = 'C'
              AND b.ACC_CODE in ('978','1365')
              AND a.remk5  BETWEEN $P{date1} AND $P{date2}

               AND b.CUST_CODE LIKE CONCAT('%',$P{t_custCode},'%')
              AND b.CUST_NAME LIKE CONCAT('%',$P{t_custName},'%')


               GROUP BY  sysID,custCode

              UNION ALL
               SELECT SYS_ID AS sysId,
                 CUST_CD AS custCode,
                 CUST_NAME as custName,
                 SUM(IFNULL(AS_COST,0)) AS "shetAmt",
                 SUM(IFNULL(TOT_TAX,0)) AS "shetTax",
                 SUM(IFNULL(TOT_AMT,0)) AS "shetTot",
                 0 as "TaxshetAmt"
                FROM as_mast
               WHERE $P{GsearchVat} != '2'
                AND SYS_ID = $P{SYS_ID}
                AND DIST_DATE BETWEEN $P{date1} AND $P{date2}

                AND CUST_CD LIKE CONCAT('%',$P{t_custCode},'%')
                AND CUST_NAME LIKE CONCAT('%',$P{t_custName},'%')
                AND SEV_CHARGE LIKE CONCAT('%',$P{salEply},'%')


            GROUP BY  SYS_ID,CUST_CD

              ) a ,CUST_MAST B
          WHERE A.sysID=B.SYS_ID
          AND A.custCode = B.CUST_CODE

          AND B.STAF_NAME LIKE CONCAT('%',$P{salEply},'%')
          AND B.CUST_TYPE LIKE CONCAT('%',$P{custType},'%')

          GROUP BY  custCode,custName
     ORDER BY custName]]>
	</queryString>
