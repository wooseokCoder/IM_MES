	<queryString>
		<![CDATA[SELECT
       SHET_NO        AS "shetNo"
      , ITEM_CODE    AS "itemCode"
     , ITEM_NAME    AS "itemName"
     , ITEM_SPEC    AS "itemSpec"
     , ITEM_UNIT    AS "itemUnit"
      , ITEM_QTY     AS "itemQty"
     , ITEM_PRCE    AS "itemPrce"
     , ITEM_PRCE * ITEM_QTY	AS "totPrce"
      , REMK         AS "remk"
      from shet_detl
      WHERE SYS_ID    = $P{SYS_ID}
    	AND SHET_TYPE  = 'B'
        AND SHET_NO = $P{hShetNo}
      ORDER  BY  SHET_NO DESC, SHET_DATE DESC]]>
