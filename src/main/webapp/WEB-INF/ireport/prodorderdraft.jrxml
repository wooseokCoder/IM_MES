	<queryString>
		<![CDATA[SELECT
       SYS_ID       AS "sysId"
     , SHET_TYPE    AS "shetType"
     , SHET_NO      AS "shetNo"
     , SHET_DATE    AS "shetDate"
     , CUST_CODE    AS "custCode"
     , CUST_NAME    AS "custName"
     , SHET_AMT     AS "shetAmt"
     , SHET_TAX     AS "shetTax"
     , SHET_TOT     AS "shetTot"
     , TOT_BAL_AMT  AS "totBalAmt"
     , TOT_COST     AS "totCost"
     , TAX_IDX      AS "taxIdx"
     , PRE_REM_IDX  AS "preRemIdx"
     , DEC_LEN      AS "decLen"
     , DISC_RATE    AS "discRate"
     , TAX_PRNT_IDX  AS "taxPrntIdx"
     , TAX_SAVE_IDX  AS "taxSaveIdx"
     , REMK1        AS "remk1"
     , USE_IDX      AS "useIdx"
     , BIZ_GP	AS "bizGp_Code"
     ,(SELECT C.CODE_NAME
		  FROM SYS_CODE C
		 WHERE SYS_ID = 'WSC'
		   AND C.CODE_GRUP = 'BSNS_PT_TYPE'
		   AND C.USE_FLAG='Y'
		   AND C.CODE_CD =  A.BIZ_GP) AS "bizGp"
     , STAF_DEPT	AS "stafDept"
     , COMP_STAF_NAME	AS "compStafName"
     , COMP_TEL     AS "compTel"
     , COMP_FAX     AS "compFax"
     , COMP_MAIL    AS "compMail"
     , CONCAT(SHET_NO,'::',SHET_TYPE) AS "shetNoType"
     , CONCAT(SHET_TYPE,SHET_NO) AS "estimateNo"
     , (SELECT SB2.USER_NAME FROM SYS_USER SB2 WHERE SB2.USER_ID = A.REGI_ID  AND SB2.SYS_ID = A.SYS_ID) AS "userName"
     ,(SELECT B.CODE_NAME
		  FROM SYS_CODE B
		 WHERE SYS_ID ='WSC'
		   AND B.CODE_GRUP = 'DEPT_CODE'
		   AND B.USE_FLAG='Y'
		   AND B.CODE_CD = A.STAF_DEPT) AS "stafDeptName"
      FROM SHET_MAST A
      WHERE SYS_ID    = $P{SYS_ID}
    	AND SHET_TYPE  = 'B'
        AND A.SHET_DATE   BETWEEN $P{date1} AND $P{date2}
        AND A.CUST_CODE LIKE CONCAT ('%',IF($P{t_custCode} IS NULL,'',$P{t_custCode}),'%')
        AND A.CUST_NAME LIKE CONCAT ('%',IF($P{t_custName} IS NULL,'',$P{t_custName}),'%')
        AND A.PROD_YN	= 'Y'
      ORDER BY SHET_DATE DESC, SHET_NO DESC]]>
	</queryString>
