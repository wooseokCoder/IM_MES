	<queryString>
		<![CDATA[SELECT
      CONCAT(SHET_NO,'::',SHET_TYPE) AS "shetNoType"
     , SHET_DATE    AS "shetDate"
     , CONCAT(SHET_TYPE,SHET_NO) AS "estimateNo"
     , (SELECT CD1.CODE_NAME FROM SYS_CODE CD1 WHERE CD1.SYS_ID = A.SYS_ID AND CD1.CODE_CD = A.STAF_DEPT AND CD1.CODE_GRUP = 'DEPT_CODE') AS "stafDeptName"
     , STAF_NAME	AS "stafName"
     , (SELECT CD2.CODE_NAME FROM SYS_CODE CD2 WHERE CD2.SYS_ID = A.SYS_ID AND CD2.CODE_CD = A.PAY_TYPE AND CD2.CODE_GRUP = 'PAY_TYPE') AS "payTypeName"
     , CUST_NAME    AS "custName"
     , SHOP_NAME    AS "shopName"
     , SHOP_STAF_NAME AS "shopStafName"
     , SHET_AMT     AS "shetAmt"
     , SHET_TAX     AS "shetTax"
     , SHET_TOT     AS "shetTot"
     , REMK1        AS "remk1"
     , COMP_TEL     AS "compTel"
     , COMP_FAX     AS "compFax"
     , COMP_MAIL    AS "compMail"
     ,(SELECT CD2.CODE_NAME FROM SYS_CODE CD2 WHERE CD2.SYS_ID = A.SYS_ID AND CD2.CODE_CD = A.UNSTOR_DEPOT AND CD2.CODE_GRUP = 'STRG_TYPE')  AS "unstorDepot"
   FROM SHET_MAST A
	WHERE A.SYS_ID    = $P{SYS_ID}
    	AND A.SHET_TYPE  = 'I'
    	AND  A.SHET_NO = $P{estimateNo}]]>
	</queryString>
