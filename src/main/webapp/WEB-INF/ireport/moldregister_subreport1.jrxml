	<parameter name="MOLD_NO" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="SYS_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
   B.ROWNUM,B.SYS_ID,B.MOLD_NO,B.MOLD_DATE,B.MOLD_CODE,B.MOLD_STAT,B.VNDR_LOC,B.MANT_COST,B.UPDT_RESN,B.MNG_REMK,B.MOLD_REMK
FROM
(
    SELECT
        @ROWNUM := @ROWNUM + 1 AS ROWNUM,
           A.SYS_ID,A.MOLD_NO,A.MOLD_DATE
,fn_get_code_name('MOLD_CODE',MOLD_CODE,'KO') AS MOLD_CODE
,fn_get_code_name('MOLD_STAT',A.MOLD_STAT,'KO') AS MOLD_STAT
,fn_get_code_name('STRG_TYPE',A.VNDR_LOC,'KO') AS VNDR_LOC
,A.MANT_COST,A.UPDT_RESN,A.MNG_REMK,A.MOLD_REMK
    FROM
        MOLD_HIST A,
       (SELECT @ROWNUM := 0) R
   WHERE SYS_ID   = $P{SYS_ID}
     AND MOLD_NO =  $P{MOLD_NO}

) B
WHERE  B.ROWNUM <= 5]]>
	</queryString>
